datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int     @id @default(autoincrement())
  username   String  @unique @db.VarChar(30)
  password   String  @db.VarChar(255)
  HighScore  Int     @default(0)
  ProfilePic String? @db.Text
}
